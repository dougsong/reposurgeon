reposurgeon: verbose 1
reposurgeon: echo 1
quiet on
# There's a --nobranch embedded in the test load so it can be checked standalone.
# This invocation would make the load work even without that.
read --nobranch <x-expunge.svn
* x-expunge
1..$ expunge /^releases\/v1.0\/.*/
reposurgeon: at 7, expunging M releases/v1.0/VERSION
choose
* x-expunge
- x-expunge-expunges
